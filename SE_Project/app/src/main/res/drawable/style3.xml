<?xml version="1.0" encoding="UTF-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="1.0px"
    android:insetRight="1.0px" >

    <selector>
        <item android:state_pressed="true">
            <shape android:shape="rectangle" >
                <gradient
                    android:angle="270.0"
                    android:endColor="#7EC7EE"
                    android:startColor="#2093CF" />

            </shape>
        </item>
        <item>
            <shape android:shape="rectangle" >
                <gradient
                    android:angle="270.0"
                    android:endColor="#7EC7EE"
                    android:startColor="#2093CF" />


            </shape>
        </item>
    </selector>
    <selector>
        <item
            android:state_pressed="false"
            >
            <shape android:shape="rectangle" >
                <gradient
                    android:angle="270.0"
                    android:endColor="#7EC7EE"
                    android:startColor="#C4E6F8" />


            </shape>
        </item>
        <item>
            <shape android:shape="rectangle" >
                <gradient
                    android:angle="270.0"
                    android:endColor="#7EC7EE"
                    android:startColor="#2093CF" />


            </shape>
        </item>
    </selector>

</inset>
